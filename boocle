# Q 12.1  What does the following snippet output ? 
let str = "1"
for (let i = 1 ; i <5 ; i++){
   if(str.length ===3){
       break;
   }
   str="0"+str
}
console.log(str)

.A : "00001" 
.B :  "001"
.C : "1"
.üëâÔ∏èD : NaN

# Q12.2 What does the following snippet output?

let s=0
let array =[10,20,30,40]
for (let i = 0 ; i <array.length ; i++){
   if(i===1){
       continue;
   }
   s=s+array[i]
}
console.log(s)

.A : 10 
.B : 60
.C : 80
.D : 100



# Q13 Which of the following for loops prints the block of code below?
                        
                               "0 5 10 15 20 25 "

//Loop 1 

for (let i = 1; i <= 25; i+= 5) {
    console.log(i)
}
//Loop 2 

for (let i = 0; i < 26; i += 5) {
    console.log(i)
}
//Loop 3 

for (let i = 0; i < 26; i += 5) {
    console.log(i)
}
//Loop 4 

for (let i = 0; i < 25; i =i+ 5) {
    console.log(i)
}
//Loop 5 

for (let i = 0; i <= 25; i =+ 5) {
    console.log(i)
}

.A : Loop 1 
.B : Loop 2 
üëâÔ∏è.C : Loop 3 
.D : Loop 4
.E : Loop 5

